{
    "name": "monomelodies/monki",
    "description": "PHP7.1+ API generation utilities",
    "type": "library",
    "keywords": [
        "php5",
        "api",
        "generation"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Marijn Ophorst",
            "email": "marijn@monomelodies.nl",
            "role": "lead"
        }
    ],
    "support": {
        "issues": "https://github.com/monomelodies/monki/issues"
    },
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "ext-pcre": "*",
        "ext-reflection": "*",
        "ext-spl": "*",
        "monolyth/reroute": "^3.6.0",
        "zendframework/zend-diactoros": "^1.2@dev",
        "league/fractal": "^0.14.0"
    },
    "require-dev": {
        "gentry/gentry": "^0.10.2"
    },
    "autoload": {
        "psr-4": {
            "Monomelodies\\Monki\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Monomelodies\\Monki\\Tests\\": "tests"
        }
    }
}
