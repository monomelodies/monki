{
    "name": "monomelodies/monki",
    "description": "PHP5 API generation utilities",
    "type": "library",
    "keywords": [
        "php5",
        "api",
        "generation"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Marijn Ophorst",
            "email": "marijn@monomelodies.nl",
            "role": "lead"
        }
    ],
    "support": {
        "issues": "https://github.com/monomelodies/monki/issues"
    },
    "require": {
        "php": ">=5.4",
        "ext-json": "*",
        "ext-pcre": "*",
        "ext-reflection": "*",
        "ext-spl": "*",
        "monomelodies/dabble": "^1.2",
        "monomelodies/improse": "^0.3.0",
        "monomelodies/reroute": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0.0",
        "phpunit/dbunit": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Monki\\": "src"
        }
    }
}
